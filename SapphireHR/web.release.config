<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <modules xdt:Transform="InsertIfMissing">
        <remove name="WebDAVModule" />
      </modules>
      <handlers>
        <remove name="WebDAV"  xdt:Transform="InsertIfMissing" />
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath="dotnet" arguments=".\SapphireHR.Web.dll" stdoutLogEnabled="true" stdoutLogFile=".\LogFiles\stdout" hostingModel="inprocess" xdt:Transform="SetAttributes(stdoutLogEnabled, stdoutLogFile)" />
    </system.webServer>
  </location>
</configuration>